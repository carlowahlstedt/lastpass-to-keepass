{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:all",
    "plugin:react/all",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "callback-return": "off",
    "no-invalid-this": "off",
    "no-magic-numbers": "off",
    "no-ternary": "off",
    "one-var": "off",
    "prettier/prettier": ["error", {"bracketSpacing": false, "singleQuote": true, "trailingComma": "es5"}],
    "react/jsx-filename-extension": "off",
    "react/no-set-state": "off"
  }
}